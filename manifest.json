{
    "manifest_version": 2,

    "name": "GCM Receiver",
    "description": "Simple GCM message receiver extension.",
    "version": "1.0",

    "permissions": [
        "http://*/*", "notifications", "gcm", "background", "storage"
    ],
    "background": {
        "scripts": ["jquery.min.js", "background.js"]
    }
}
